<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="ExampleApplicationWindow" parent="AdwApplicationWindow">
    <property name="default-width">600</property>
    <property name="default-height">400</property>
    <child>
      <object class="AdwLeaflet" id="leaflet">
        <property name="can-swipe-back">True</property>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <property name="width-request">280</property>
            <child>
              <object class="AdwHeaderBar" id="sidebar_headerbar">
                <property name="show-end-title-buttons">False</property>
                <property name="show-start-title-buttons">False</property>
              </object>
            </child>
            <child>
              <object class="GtkScrolledWindow">
                <property name="vexpand">True</property>
                <child>
                  <object class="GtkStackSidebar">
                    <property name="stack">stack</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwLeafletPage">
            <property name="navigatable">False</property>
            <property name="child">
              <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkWindowHandle" id="header_separator">
                    <child>
                      <object class="GtkSeparator">
                        <property name="orientation">vertical</property>
                        <style>
                          <class name="sidebar"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkSeparator">
                    <property name="orientation">vertical</property>
                    <property name="vexpand">True</property>
                    <style>
                      <class name="sidebar"/>
                    </style>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="AdwHeaderBar" id="main_headerbar">
                <property name="title-widget">
                  <object class="GtkLabel" id="title_label">
                    <property name="single-line-mode">True</property>
                    <property name="ellipsize">end</property>
                    <property name="width-chars">5</property>
                    <style>
                      <class name="title"/>
                    </style>
                  </object>
                </property>
                <child type="start">
                  <object class="GtkRevealer">
                    <property name="transition-type">crossfade</property>
                    <property name="reveal-child" bind-source="leaflet" bind-property="folded" bind-flags="sync-create" />
                    <child>
                      <object class="GtkButton">
                        <property name="icon-name">go-previous-symbolic</property>
                        <property name="action-name">win.back</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkScrolledWindow">
                <property name="min-content-width">300</property>
                <property name="hscrollbar-policy">never</property>
                <property name="min-content-height">250</property>
                <property name="vexpand">True</property>
                <child>
                  <object class="AdwClamp">
                    <property name="hexpand">True</property>
                    <child>
                      <object class="GtkStack" id="stack">
                        <property name="vhomogeneous">False</property>
                        <property name="margin-top">24</property>
                        <property name="margin-bottom">24</property>
                        <property name="margin-start">12</property>
                        <property name="margin-end">12</property>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">account</property>
                            <property name="title">Account</property>
                            <property name="child">
                              <object class="AccountPage" id="account" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">background</property>
                            <property name="title">Background</property>
                            <property name="child">
                              <object class="BackgroundPage" id="background" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">camera</property>
                            <property name="title">Camera</property>
                            <property name="child">
                              <object class="CameraPage" id="camera" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">device</property>
                            <property name="title">Device</property>
                            <property name="child">
                              <object class="DevicePage" id="device" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">email</property>
                            <property name="title">Email</property>
                            <property name="child">
                              <object class="EmailPage" id="email" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">file_chooser</property>
                            <property name="title">File Chooser</property>
                            <property name="child">
                              <object class="FileChooserPage" id="file_chooser" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">inhibit</property>
                            <property name="title">Inhibit</property>
                            <property name="child">
                              <object class="InhibitPage" id="inhibit" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">location</property>
                            <property name="title">Location</property>
                            <property name="child">
                              <object class="LocationPage" id="location" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">network_monitor</property>
                            <property name="title">Netowkr Monitor</property>
                            <property name="child">
                              <object class="NetworkMonitorPage" id="network_monitor" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">notification</property>
                            <property name="title">Notifications</property>
                            <property name="child">
                              <object class="NotificationPage" id="notification" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">open_uri</property>
                            <property name="title">Open URI</property>
                            <property name="child">
                              <object class="OpenUriPage" id="open_uri" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">screenshot</property>
                            <property name="title">Screenshot</property>
                            <property name="child">
                              <object class="ScreenshotPage" id="screenshot" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">screencast</property>
                            <property name="title">ScreenCast</property>
                            <property name="child">
                              <object class="ScreenCastPage" id="screencast" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">wallpaper</property>
                            <property name="title">Wallpaper</property>
                            <property name="child">
                              <object class="WallpaperPage" id="wallpaper" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">secret</property>
                            <property name="title">Secret</property>
                            <property name="child">
                              <object class="SecretPage" id="secret" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">documents</property>
                            <property name="title">Documents</property>
                            <property name="child">
                              <object class="DocumentsPage" id="documents" />
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="name">welcome</property>
                            <property name="child">
                              <object class="AdwStatusPage">
                                <property name="title">Welcome to ASHPD Demo</property>
                                <property name="description">Play with portals</property>
                                <property name="icon-name">portals-symbolic</property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <object class="GtkSizeGroup">
      <property name="mode">vertical</property>
      <widgets>
        <widget name="sidebar_headerbar"/>
        <widget name="main_headerbar"/>
        <widget name="header_separator"/>
      </widgets>
    </object>
  </template>
</interface>
