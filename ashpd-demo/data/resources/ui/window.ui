<interface>
  <menu id="primary_menu">
    <section>
      <item>
        <attribute name="label" translatable="yes">_Preferences</attribute>
        <attribute name="action">app.preferences</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">_Keyboard Shortcuts</attribute>
        <attribute name="action">win.show-help-overlay</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">_About GTK Rust Template</attribute>
        <attribute name="action">app.about</attribute>
      </item>
    </section>
  </menu>
  <template class="ExampleApplicationWindow" parent="GtkApplicationWindow">
    <property name="default-width">600</property>
    <property name="default-height">400</property>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="headerbar">
        <child type="end">
          <object class="GtkMenuButton" id="appmenu_button">
            <property name="icon-name">open-menu-symbolic</property>
            <property name="menu-model">primary_menu</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="orientation">horizontal</property>
        <child>
          <object class="GtkListBox" id ="listbox">
            <property name="selection-mode">single</property>
            <property name="width-request">200</property>
            <child>
              <object class="SidebarRow">
                <property name="label">Account</property>
                <property name="page-name">account</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Background</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Camera</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Device</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Email</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">File Chooser</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Inhibit</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Location</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Network Monitor</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Notification</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Open URI</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Print</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Remote Desktop</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Screen Cast</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Screenshot</property>
                <property name="page-name">screenshot</property>
              </object>
            </child>
            <child>
              <object class="SidebarRow">
                <property name="label">Wallpaper</property>
              </object>
            </child>
            <style>
              <class name="navigation-sidebar"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkSeparator">
            <property name="orientation">vertical</property>
          </object>
        </child>
        <child>
          <object class="GtkStack" id="stack">
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <property name="margin-start">12</property>
            <property name="margin-end">12</property>
            <child>
              <object class="GtkStackPage">
                <property name="title">ScreenShot</property>
                <property name="child">
                  <object class="ScreenshotPage" id="screenshot" />
                </property>
              </object>
            </child>
            <child>
              <object class="GtkStackPage">
                <property name="title">User Information</property>
                <property name="child">
                  <object class="GtkBox" id="account_info">
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkStackPage">
                <property name="name">welcome</property>
                <property name="child">
                  <object class="GtkBox">
                    <child>
                      <object class="GtkLabel">
                        <property name="label">Welcome page</property>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
